{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "cosmetic-market-ui"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico",
        "robots.txt"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "styles": [
        "styles.css",
        "../node_modules/bootstrap/dist/css/bootstrap.css",
        "../src/assets/libs/js/datatables/datatables.net-bs/css/dataTables.bootstrap.css",
        "../src/assets/libs/js/datatables/datatables.net-select-bs/css/select.bootstrap.css",
        "../src/assets/libs/js/datatables/datatables.net-dt/css/jquery.dataTables.css"
      ],
      "scripts": [
        "../node_modules/jquery/dist/jquery.min.js",
        "../node_modules/popper.js/dist/umd/popper.min.js",
        "../node_modules/bootstrap/dist/js/bootstrap.js",
        "../node_modules/bootstrap/dist/js/bootstrap.bundle.min.js",
        "../node_modules/jquery.easing/jquery.easing.min.js",
        "../node_modules/jquery.scrollto/jquery.scrollTo.min.js",
        "../node_modules/owl.carousel/dist/owl.carousel.min.js",
        "../node_modules/wowjs/dist/wow.min.js",
        "../src/assets/libs/js/jquery-ui.min.js",
        "../node_modules/blueimp-file-upload/js/jquery.fileupload.js",
        "../src/assets/libs/js/iphone-inline-video/iphone-inline-video.js",
        "../src/assets/libs/js/vide/jquery.vide.js",
        "../src/assets/libs/js/background-video.js",
        "../src/assets/libs/js/aws-client.js",
        "../src/assets/libs/js/custom.js",
        "../src/assets/libs/js/datatables/datatables.net/js/jquery.dataTables.js",
        "../src/assets/libs/js/datatables/datatables.net-bs/js/dataTables.bootstrap.js",
        "../src/assets/libs/js/datatables/datatables.net-select/js/dataTables.select.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "src/tsconfig.spec.json",
      "exclude": "**/node_modules/**"
    },
    {
      "project": "e2e/tsconfig.e2e.json",
      "exclude": "**/node_modules/**"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
